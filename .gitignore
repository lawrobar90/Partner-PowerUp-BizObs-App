configure-dynatrace.sh
apply-dynatrace-config.sh
*dynatrace-config*
*oneagent*
*.dt-config

# SSH Keys and sensitive files
*.pem
*.key
*_key
*_key.pub
perform2026_new_key*

# Environment and deployment files
.env*
*.env
deploy-*.sh
cloud-deploy-*.sh

# System configuration files
.bash_logout
.bash_profile
.bashrc
.viminfo
.gitconfig

# Project specific scripts that shouldn't be committed
monitor_service_usage.sh
analyze_service_usage.sh
test-*.sh
restart-*.sh
start-all
start-bizobs
stop-all
stop-bizobs
stop-vegas
project-control.sh
fix_vscode_cache.sh
nginx-setup.sh
python-bizobs.py
simple-bizobs.js
ultra-simple.html
journey-payload.json

